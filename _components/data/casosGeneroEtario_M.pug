//- INGRESA DATA
script.
	$( document ).ready(function(){
		setTimeout(function(){

		var ultimaFecha = $( "#casosGeneroEtario_F table tr" ).find( "th" ).last().html();
		$( "#casosGeneroEtario_F-grafico p.ultimaActualizacion span" ).html( ultimaFecha );

		for ( sumaTR = 1 ; sumaTR <= $( "#casosGeneroEtario_F table" ).find( "tr" ).length-2 ; sumaTR++  ) {

			var edad 		= $( $( $( "#casosGeneroEtario_F table" ).find( "tr" )[sumaTR] ).find( "td" )[0] ).first().html();

			$( ".grafico#casosGeneroEtario_F-grafico > div > ul" ).append( "" +
				"<li>" + edad + "</li>" +
			"" );

		};

		},1000);