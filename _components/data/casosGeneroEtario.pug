script.
	traeCovid( "#casosGeneroEtario" , "https://raw.githubusercontent.com/MinCiencia/Datos-COVID19/master/output/producto16/CasosGeneroEtario.csv" );

//- INGRESA DATA
script.
	$( document ).ready(function(){
		setTimeout(function(){

		$( "#casosGeneroEtario-grafico p.ultimaActualizacion span" ).html( $( "#casosGeneroEtario table tr" ).find( "th" ).last().html() + "*" );

		for ( sumaTR = 1 ; sumaTR <= $( "#casosGeneroEtario table" ).find( "tr" ).length-1 ; sumaTR++  ) {

			var edad 	= $( $( $( "#casosGeneroEtario table" ).find( "tr" )[sumaTR] ).find( "td" )[0] ).first().html();
			var casos	= $( $( "#casosGeneroEtario table" ).find( "tr" )[sumaTR] ).find( "td" ).last().html();

			$( ".grafico#casosGeneroEtario-grafico > div.femenino > ul" ).append( "<li>" + edad + ": " + casos + "</li>" );
			$( ".grafico#casosGeneroEtario-grafico > div.masculino > ul" ).append( "<li>" + edad + ": " + casos + "</li>" );

		};

		},1000);
	});