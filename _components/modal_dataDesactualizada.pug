	script.
		$( document ).ready(function(){

			setTimeout(function(){

				var fechaDiaReporte 				= $( "#TotalesNacionales th" ).last().html().split( "-" )[2]
				if( fechaDiaReporte.split( "" ).length == 2 && fechaDiaReporte.split( "" )[0] == 0 ) {
					var fechaDiaReporte = fechaDiaReporte.split( "" )[1];
				};

				var fechaMesReporte 				= $( "#TotalesNacionales th" ).last().html().split( "-" )[1]
				if( fechaMesReporte.split( "" ).length == 2 && fechaMesReporte.split( "" )[0] == 0 ) {
					var fechaMesReporte = fechaMesReporte.split( "" )[1];
				};

				var fechaActual 		= new Date().getDate();

				$( "div.modal-contenido p span.dia" ).html( fechaDiaReporte + "/" + fechaMesReporte + "/2020" );

				if( fechaDiaReporte != fechaActual && new Date().getHours() >= "8" ) {
					$( ".modal-fondo" ).show();
					$( ".modal-contenido" ).show();
				};
			
			},1000);
			
		});

		$( ".modal-fondo, .modal-contenido > span" ).click(function(){
			$( "div.modal-fondo" ).hide();
			$( "div.modal-contenido" ).hide();
		});
