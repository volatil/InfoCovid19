						const estadoDarkMode = () => {
							if ( localStorage.getItem( "darkmode" ) ) {
								if ( localStorage.getItem( "darkmode" ) == "activado" ) {
									console.log( `Estamos activo paaapiii !` );
									$( "body" ).addClass( "darkmode-si" );
									$( ".toggle-container .label" ).click()
								} else {
									console.log( `Buuuu ... estamOFF` );
								};
							} else {
								localStorage.setItem( "darkmode" , "desactivado" );
							};
						};
							estadoDarkMode();
						
						//- CLICK MENU DARKMODE
						$( ".toggle-container > label" ).click(function(){
							var estado = $( ".toggle-container > input" ).prop( "checked" );
							if ( estado == false ) {
								//- document.cookie = "darkmode=activado";
								$( "body" ).addClass( "darkmode-si" );
								localStorage.setItem( "darkmode" , "activado" );
							} else {
								//- document.cookie = "darkmode=desactivado";
								$( "body" ).removeClass( "darkmode-si" );
								localStorage.setItem( "darkmode" , "desactivado" );
							};
						});
				
						//- ANUNCIO DEL DARKMODE
						if( window.innerWidth <= 876 ) {
							var posicionMenu = "derecha";
						} else {
							var posicionMenu = "izquierda";
						};
						$( "body" ).append( `
							<style type="text/css">
								body .anuncio {
									position: fixed;
									bottom: 10px;
									z-index: 2;
									width: 590px;
									margin: 0 auto;
									left: 0;
									right: 0;
									background: cyan;
									color: black;
									text-align: center;
									border-radius: 6px;
								}
								@media ( max-width: 600px ) {
									body .anuncio {
										width: 90%;
										font-size: .8em;
										padding: 0 5px;
									}
								}
							</style>
							<div class="anuncio">
								<p>Ahora puedes disfrutar del <strong>Darkmode</strong>, lo encontraras en el menu de la <span>${posicionMenu}</span></p>
							</div>
						` );
						setTimeout(function(){
							$( "body .anuncio" ).fadeOut();
						},5000);
				
				
				
				

div.fondoNav

	script.
		// NAV
		$( ".menuIcon" ).click(function(){
			if ( $( ".menuIcon" ).attr( "class" ).includes( "activo" ) ) {
				$( this ).removeClass( "activo" );
				$( "div.fondoNav" ).removeClass( "activo" );
				$( "header nav" ).removeClass( "activo" );
			} else {
				$( this ).addClass( "activo" );
				$( "div.fondoNav" ).addClass( "activo" );
				$( "header nav" ).addClass( "activo" );
				$( "header nav" ).height( $( window ).height()-100 );
				$( "header nav" ).height( $( document ).height() );
			};
		});
		$( ".fondoNav" ).click(function(){
			$( ".menuIcon" ).removeClass( "activo" );
			$( "div.fondoNav" ).removeClass( "activo" );
			$( "header nav" ).removeClass( "activo" );
		});
		// NAV => LUGAR ACTIVO
		$( document ).ready(function(){

			if ( location.href.includes( "dashboard" ) ) {

				$( "header nav ul li.dashboard" ).addClass( "activo" );
				$( "header h1" ).html( "DASHBOARD" );

			} else if ( location.href.includes( "dev" ) ) {

				$( "header nav ul li.dev" ).addClass( "activo" );
				$( "header h1" ).html( "EXPERIMENTOS" );

			} else if ( location.href.includes( "contacto" ) ) {

				$( "header nav ul li.contacto" ).addClass( "activo" );
				$( "header h1" ).html( "CONTACTO" );

			} else {

				$( "header nav ul li.inicio" ).addClass( "activo" );
				$( "header h1" ).html( "" );

			};

		});

